{
  "$schema": "https://raw.githubusercontent.com/vanessa-opensource/vanessa-runner/develop/behavior-schema.json",
  "ИмяСборки": "VanessaAutomationSingle",
  "ВыполнитьСценарии": true,
  "СтрокаПодключенияКБазе": "/F./build/ib",
  "ПутьКVanessaBehavior": "./tools/vanessa-automation-single.epf",
    "ЗавершитьРаботуСистемы": true,
    "ЗакрытьTestClientПослеЗапускаСценариев": true,
    "КаталогФич": "$workspaceRoot/features",
    "ДелатьОтчетВФорматеАллюр": true,
    "КаталогOutputAllureБазовый": "$workspaceRoot/build/out/allure",
    "ВыводитьСообщенияВФайл": "$workspaceRoot/build/automation-log.txt",
    "ВыгружатьСтатусВыполненияСценариевВФайл": true,
    "ПутьКФайлуДляВыгрузкиСтатусаВыполненияСценариев": "$workspaceRoot/build/BuildStatus.log",
    "ВыводитьВЛогВыполнениеШагов": true,
    "ИмяКаталогаЛогОшибок": "$workspaceRoot/build/reports/errors",
    "ДелатьЛогВыполненияСценариевВТекстовыйФайл": true,
    "DebugLog": false,
    "ЗапускатьТестКлиентВРежимеОтладки": true,
    "ДелатьЛогВыполненияСценариевВЖР": true,    
    "ТаймаутЗапуска1С": "25",
    "ЯзыкГенератораGherkin": "ru",
    "ПутьКФайлуДляВыгрузкиСтатусаВыполненияСценариев": "$workspaceRoot/build/BuildStatus.log",
    "КаталогOutputAllure": "$workspaceRoot/build/reports/allure",
    "КаталогOutputAllureБазовый": "$workspaceRoot/build/reports/allure",
    "КаталогOutputjUnit": "$workspaceRoot/build/reports/junit",
    "КаталогOutputCucumberJson": "$workspaceRoot/build/reports/cucumber",
    "ИмяФайлаЛогВыполненияСценариев": "$workspaceRoot/build/log.txt",
    "КомандаСделатьСкриншот": "\"C:\\Program Files\\IrfanView\\i_view64.exe\" /capture=1 /convert=",
    "ВыполнятьШагиАссинхронно": false,
    "ПриоритетНастроекПользователя": true,
    "ДелатьСкриншотПриВозникновенииОшибки": true,
    "КаталогOutputСкриншоты": "$workspaceRoot/build/reports/ScreenShots",
    "ВерсияПлатформыДляГенерацииEPF": "",
    "ДиапазонПортовTestclient": "48000-48100",
    "ПутьКVanessaADD": "./tools/vanessa-automation-single.epf", 
    "ПутьКVanessa": "./tools/vanessa-automation-single.epf",
    "СписокТеговИсключение": ["Draft","IgnoreOnCIMainBuild"]
  }
  